<%= form_for @user do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :height %>
  <%= f.text_field :height %>
  <%= f.label :happiness %>
  <%= f.text_field :happiness %>
  <%= f.label :nausea %>
  <%= f.text_field :nausea %>
  <%= f.label :tickets %>
  <%= f.text_field :tickets %>
  <%= f.label :admin %>
  <%= f.check_box :admin %>
  <%= f.submit "Create User"%>
<% end %>
